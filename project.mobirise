            "<script>\n        var k_form_rand_130=Math.random()+'_'+(new Date()).getTime(); // Уникальный id формы, используется в файлах и для получения ответа анкеты\n        var k_upload_in_progress=0;\n        var k_answer_hide_form130=1;\n        var k_was_submited130=0;\n        var k_130_curr_hash = '';\n        var k_link_behavior = 1;\n\n        \n\n        \n\n        \n\n        \n        // Загрузка файла\n        var k_upload_files_list;\n\n        function k_add_file(obj) {\n            var page_charset=window.document.charset;\n            if (!page_charset) page_charset=window.document.characterSet;\n            var i_c=obj.getAttribute('i_c');\n            var field_id=obj.getAttribute('field_id');\n            var value=obj.value;\n            var progress_span=\"<span class='k_upload_progress k_upload_progress_img'></span>\";\n            k_upload_files_list=[];\n\n            if (obj.files) { // Новый режим многофайловость\n                var i;\n                for (i=0;i<obj.files.length;i++) {\n                    value=obj.files[i].fileName;\n                    if (typeof(value)=='undefined') value=obj.files[i].name;\n                    var new_line=$(\"<div class='k_upload_res' data-value='\"+field_id+'_'+value+\"'>\"+value+progress_span+\"</div>\");\n                    var m_p =document.getElementById('add_file_url'+i_c);\n                    $(new_line).insertBefore(m_p);\n                    var f_info=new Object();\n                    f_info.name=value;\n                    f_info.obj=new_line;\n                    f_info.field_id=field_id;\n                    k_upload_files_list.push(f_info);\n                }\n            } else {  // Старый режим\n                // Если указан полный путь оставляем только имя файла\n                var last_slash=-1;\n                var last_slash_p1=0;\n                var last_slash_p2=-1;\n                while (1)\n                {\n                    last_slash_p2=value.indexOf('\\\\',last_slash_p1);\n                    if (last_slash_p2==-1) break;\n                    last_slash_p1=last_slash_p2+1;\n                    last_slash=last_slash_p2;\n                }\n                if (last_slash!=-1)\n                {\n                    value=value.substr(last_slash+1,1024*1024);\n                };\n                var new_line=$(\"<div class='k_upload_res' data-value='\"+field_id+\"_\"+value+\">\"+value+progress_span+\"</div>\");\n                var m_p =document.getElementById('add_file_url'+i_c);\n                $(new_line).insertBefore(m_p);\n                var f_info=new Object();\n                f_info.name=value;\n                f_info.obj=new_line;\n                f_info.field_id=field_id;\n                k_upload_files_list.push(value);\n            }\n\n            k_upload_in_progress=1;\n            document.getElementById('k_sbmt_file'+i_c).action=\"//crm129588.clientbase.ru/questionare.php?sel=save_file&qst_id=130&field_id=\"+field_id+\"&page_charset=\"+page_charset+\"&k_rand=\"+k_form_rand_130;\n            document.getElementById('k_sbmt_file'+i_c).submit();\n        }\n\n        function k_del_file(obj) {\n            var field_id = obj.getAttribute('field_id');\n            var f_name = obj.getAttribute('f_name');\n            var page_charset=window.document.charset;\n            if (!page_charset) page_charset=window.document.characterSet;\n            document.getElementById('k_sbmt_file_130_'+field_id).action='https://localhost/cb_distr/questionare.php?sel=del_file&qst_id=130&field_id='+field_id+'&f_name='+encodeURIComponent(f_name)+'&page_charset='+page_charset+'&k_rand='+k_form_rand_130;\n            document.getElementById(\"k_sbmt_file_130_\"+field_id).submit();\n            $('[data-value = \"'+field_id+\"_\"+f_name+'\"]').remove();\n            var was_upload = +document.getElementById('k_sbmt_file_130_'+field_id).getAttribute('was_upload');\n            document.getElementById('k_sbmt_file_130_'+field_id).setAttribute('was_upload', was_upload - 1);\n        }\n    \n\n        function k_save_qst130()\n        {\n            k_answer_hide_form130=1;\n            var page_charset=window.document.charset;\n            if (!page_charset) page_charset=window.document.characterSet;\n\n            if (typeof(custom_save_qst130) == 'function') {\n                if (!custom_save_qst130()) return ;\n            }\n\n            if (k_was_submited130) return;\n            k_was_submited130=1;\n            document.getElementById('k_savebutton130').disabled=true;\n            document.getElementById('k_savebutton130').style.cursor='default';\n\n            var qst_form=document.createElement('form');\n            qst_form.setAttribute('enctype', 'multipart/form-data');\n            qst_form.setAttribute('action',  '//crm129588.clientbase.ru/questionare.php?page_charset='+page_charset+'&ts'+new Date().getTime());\n            qst_form.setAttribute('target',  'k_frame_upload_130');\n            qst_form.setAttribute('method',  'post');\n            document.body.appendChild(qst_form);\n            var qst_input;\n            var utm_href = window.location.search;\n            qst_input=document.createElement('input');qst_input.setAttribute('type','hidden');qst_input.setAttribute('name','utm_href');qst_form.appendChild(qst_input);\n            qst_input.value=utm_href;\n\n            qst_input=document.createElement('input');qst_input.setAttribute('type','hidden');qst_input.setAttribute('name','k_input_field_130_20420');qst_form.appendChild(qst_input);\nqst_input.value=document.getElementById('k_input_field_130_20420').value;\nqst_input=document.createElement('input');qst_input.setAttribute('type','hidden');qst_input.setAttribute('name','k_input_field_130_20430');qst_form.appendChild(qst_input);\nqst_input.value=document.getElementById('k_input_field_130_20430').value;\n;\n\n            if(k_130_curr_hash != '') {\n                qst_input=document.createElement('input');qst_in