                "@media (max-width: 992px)": {
                  "padding": "0 6px"
                }
              }
            },
            ".container": {
              "@media (max-width: 992px)": {
                "padding": "0 6px"
              }
            },
            ".row-main": {
              "margin": "0",
              "padding": "0 150px",
              "justify-content": "center",
              "@media (max-width: 1440px)": {
                "padding": "0 32px"
              },
              "@media (max-width: 1200px)": {
                "padding": "0 8px"
              },
              "@media (max-width: 992px)": {
                "padding": "0 60px"
              },
              "@media (max-width: 768px)": {
                "padding": "0 30px"
              },
              ".card": {
                "padding": "0",
                "border-radius": "0 !important",
                "overflow": "visible"
              }
            },
            ".form-wrap": {
              "padding": "40px 45px ",
              "background-color": "@form",
              "border-radius": "10px",
              "@media (max-width: 992px)": {
