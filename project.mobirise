              },
              ".dragArea.row": {
                ".mbr-section-title": {
                  "margin-bottom": "32px !important"
                },
                ".mbr-text": {
                  "margin-bottom": "32px"
                },
                ".form-group": {
                  "margin-bottom": "25px !important",
                  ".form-control": {
                    "padding": "0 20px",
                    "min-height": "60px",
                    "border": "none !important",
                    "box-shadow": "none",
                    "border-radius": "10px !important",
                    "background-color": "@input",
                    "&:hover, &:focus": {
